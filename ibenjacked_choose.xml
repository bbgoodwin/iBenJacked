<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="42dp"
        android:layout_height="27dp"
        android:layout_alignStart="@+id/repsText"
        android:layout_alignTop="@+id/imageButton"
        app:srcCompat="@drawable/ic_launcher_background" />

    <Button
        android:id="@+id/cancelButton"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:layout_alignEnd="@+id/workout1"
        android:layout_alignTop="@+id/okButton"
        android:text="cancel" />

    <EditText
        android:id="@+id/repsText"
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/workoutText2"
        android:layout_alignTop="@+id/setsText"
        android:backgroundTint="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="REPS" />

    <EditText
        android:id="@+id/workoutText2"
        android:layout_width="182dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/workout1"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="33dp"
        android:layout_marginTop="14dp"
        android:backgroundTint="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:selectAllOnFocus="true"
        android:text="Choose Your Workout" />

    <Button
        android:id="@+id/workoutbutton3"
        android:layout_width="247dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="186dp"
        android:text="Workout 3" />

    <Button
        android:id="@+id/workoutbutton2"
        android:layout_width="247dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:text="Workout 2" />

    <Button
        android:id="@+id/workout1"
        android:layout_width="247dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="68dp"
        android:layout_marginTop="75dp"
        android:text="Workout 1" />

    <EditText
        android:id="@+id/setsText"
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="210dp"
        android:layout_toEndOf="@+id/backButton"
        android:backgroundTint="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="SETS" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="19dp"
        android:layout_marginStart="17dp"
        android:text="Back" />

    <Button
        android:id="@+id/finishButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/backButton"
        android:layout_marginEnd="24dp"
        android:text="Finish" />

    <Button
        android:id="@+id/okButton"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="121dp"
        android:layout_marginStart="78dp"
        android:text="OK" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="42dp"
        android:layout_height="27dp"
        android:layout_alignEnd="@+id/setsText"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="169dp"
        app:srcCompat="@drawable/ic_launcher_background" />


</RelativeLayout>